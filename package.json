{
	"name": "t1-tecnhical-test",
	"private": true,
	"workspaces": [
		"frontend",
		"backend"
	],
	"scripts": {
		"dev": "concurrently \"npm:dev:front\" \"npm:dev:back\"",
		"dev:front": "npm --workspace frontend run dev",
		"dev:back": "npm --workspace backend run dev",
		"test": "npm --workspace frontend run test",
		"test:watch": "npm --workspace frontend run test:watch",
		"test:coverage": "npm --workspace frontend run test:coverage"
	},
	"devDependencies": {
		"concurrently": "^8.2.0"
	},
	"dependencies": {
		"@heroicons/react": "^2.2.0"
	}
}
